    "commander": "^2.19.0",
    "nunjucks": "^3.1.7",
    "typescript": "^3.2.2"
  },
  "devDependencies": {
    "eslint": "^5.12.0",
    "eslint-config-prettier": "^3.4.0",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-typescript": "^0.14.0",
    "prettier": "^1.15.3",
    "typescript-eslint-parser": "^21.0.2"
